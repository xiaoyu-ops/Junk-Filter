# TrueSignal 环境变量配置示例

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=truesignal
DB_PASSWORD=truesignal123
DB_NAME=truesignal

# Redis 配置
REDIS_URL=redis://localhost:6379/0

# 日志配置
LOG_LEVEL=INFO

# LLM 配置（yu_agent框架）
# 使用 OpenAI 时：
OPENAI_API_KEY=sk-xxxxx
LLM_MODEL_ID=gpt-4
# 或使用其他提供商：
# DEEPSEEK_API_KEY=sk-xxxxx
# DASHSCOPE_API_KEY=xxxx （阿里云通义）
# LLM_MODEL_ID=qwen-plus

# Go 后端配置
GO_SERVER_PORT=8080
